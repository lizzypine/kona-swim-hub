container_commands:
  01_collectstatic:
    command: "source $PYTHONPATH/activate && python manage.py collectstatic --noinput"
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: kona-swim-hub.wsgi:application
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static